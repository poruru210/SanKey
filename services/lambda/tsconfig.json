{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "dist",
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "lib": ["ES2020","DOM"],
    "types": ["node","aws-lambda","jest"],
    "esModuleInterop": true
  },
  "include": ["src/**/*","tests/**/*"],
  "exclude": ["node_modules","dist"]
}
